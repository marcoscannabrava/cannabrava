{
  "name": "cannabrava.co",
  "private": true,
  "description": "A starter for a blog powered by Gatsby and Markdown",
  "version": "1.0.0",
  "author": "Marcos Cannabrava <mpcannabrava@gmail.com>",
  "bugs": {
    "url": "https://github.com/marcoscannabrava/cannabrava"
  },
  "dependencies": {
    "@fontsource-variable/montserrat": "^5.0.17",
    "@fontsource/merriweather": "^5.0.8",
    "gatsby": "^5.14.0",
    "gatsby-plugin-feed": "^5.14.0",
    "gatsby-plugin-image": "^3.14.0",
    "gatsby-plugin-manifest": "^5.14.0",
    "gatsby-plugin-react-svg": "^3.3.0",
    "gatsby-plugin-sharp": "^5.14.0",
    "gatsby-remark-images": "^7.14.0",
    "gatsby-remark-prismjs": "^7.14.0",
    "gatsby-remark-responsive-iframe": "^6.14.0",
    "gatsby-source-filesystem": "^5.14.0",
    "gatsby-transformer-remark": "^6.14.0",
    "gatsby-transformer-sharp": "^5.14.0",
    "prismjs": "^1.29.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "gh-pages": "^6.2.0",
    "prettier": "^2.8.8"
  },
  "homepage": "https://cannabrava.co",
  "license": "0BSD",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcoscannabrava/cannabrava.git"
  },
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "add-domain": "echo 'cannabrava.co' > public/CNAME",
    "deploy": "gatsby build --prefix-paths && npm run add-domain && gh-pages -d public"
  }
}
